<div class='container'>

  <h1><%= @project.title %></h1>
  <p><%= link_to 'Back', projects_path %> </p>


  <div class="row">

    <div class="col-md-3" id="admin-side-panel">
      <%= render 'shared/admin_sidepanel' %>
    </div>

    <div class="col-md-9" id='admin-main-panel'>

      <% if !flash[:notice].blank? %>
        <div class="alert alert-success" role="success">
          <%= flash[:notice] %>
        </div>
       <% end %>

      <%= render 'taskflows/taskflow_admin_panel' %>
    </div>

  </div>

</div>
